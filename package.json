{
  "name": "codescannerreport",
  "displayName": "CodeScannerReport",
  "description": "Extensão para reportar anomalidades no código",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codescannerreport.scanFiles",
        "title": "CodeScanner"
      }
    ],
    "keybindings": [
			{
				"command": "codescannerreport.scanFiles",
				"key": "ctrl+alt+z",
				"mac": "cmd+alt+z",
				"when": "true"
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/pdfkit": "^0.13.4",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "pdfkit": "^0.15.0"
  },
  "publisher": "AdrianoXavier",
  "icon": "img/logo.jpeg"
}
